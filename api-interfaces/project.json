{
  "name": "api-interfaces",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "api-interfaces/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/api-interfaces",
        "main": "api-interfaces/src/index.ts",
        "tsConfig": "api-interfaces/tsconfig.lib.json",
        "assets": ["api-interfaces/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["api-interfaces/**/*.ts", "api-interfaces/package.json"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "api-interfaces/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
